{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "wdio.conf.js"
    ]
  },
  "rules": {
    "interface-name": false,
    "ordered-imports": false,
    "no-empty-interface": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false,
    "no-console": false,
    "no-empty": false
  }
}
